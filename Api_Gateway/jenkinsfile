pipeline { 
    agent any  
    stages { 
        stage('Build') { 
            steps {
                sh "${mavenPath} -Precette clean install -DJAVA_HOME=${javaHome}"
                }
        } 
        stage('SonarQube analysis') {
            steps {
                withSonarQubeEnv('My SonarQube Server') {
                    sh "${mavenPath} -Psonar sonar:sonar"
                }
              
            }
     
        }
