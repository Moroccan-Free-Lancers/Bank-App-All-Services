node {
  stage('SCM') {
    checkout scm
  }
stage("SonarQube analysis") {
        steps {
            script {
                def scannerHome = tool 'sonarqube';
                withSonarQubeEnv('sonarqubeServer') {
                    sh 'mvn clean package sonar:sonar'
                }
            }
        }
    }
}
