node {
  stage('SCM') {
    checkout scm
  }
  stage('SonarQube Analysis') {
                def mvn = tool 'MAVEN_HOME';
                withSonarQubeEnv('sonarqube') {
                    sh "${mvn}/bin/mvn clean verify sonar:sonar -Dsonar.projectKey=Backend-test -Dsonar.login=admin -Dsonar.password=lamya2210"
                }
  
}
}
