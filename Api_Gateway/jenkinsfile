pipeline { 
    agent any  
    stages { 
          stage('SCM') {
    checkout scm
  }
        } 
        stage('SonarQube analysis') {
            steps {
                withSonarQubeEnv('My SonarQube Server') {
                    sh "${mavenPath} -Psonar sonar:sonar"
                }
              
            }
     
        }
        }
